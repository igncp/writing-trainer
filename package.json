    "@types/react-helmet": "^6.1.5",
    "csv-loader": "^3.0.3",
    "i18next": "^23.11.5",
    "i18next-http-backend": "^2.5.2",
    "i18next-parser": "^9.0.0",
    "lodash": "^4.17.21",
    "next": "^14.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^14.1.2",
    "react-icons": "^5.2.1",
