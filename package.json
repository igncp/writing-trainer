{
  "name": "writing-trainer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run next:build && bash scripts/package_extension.sh",
    "check": "bash scripts/check.sh",
    "clean": "sh scripts/clean.sh",
    "eslint": "eslint . --ext ts --ext tsx --ext js",
    "format": "bash scripts/format.sh",
    "gen:gql": "graphql-codegen",
    "next:build": "next build",
    "next:develop": "next dev",
    "parse-translations": "i18next 'src/**/*.{ts,tsx}'",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:check": "npm run prettier -- --list-different",
    "prettier:fix": "npm run prettier -- --write",
    "test": "jest",
    "type-coverage": "typescript-coverage-report --ignore-files '**/*.json'"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
