{
  "name": "writing-trainer-chrome-extension",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "sh helpers/package.sh",
    "build-storybook": "build-storybook",
    "eslint": "eslint . --ext ts --ext tsx --ext js",
    "format": "sh helpers/format.sh",
    "pre-commit": "sh helpers/gitHooks/precommit.sh",
    "pre-push": "sh helpers/gitHooks/prepush.sh",
    "sonar": "npm test -- --coverage && SONAR_TOKEN=$(cat sonar-token.txt) node helpers/sonar.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest --runInBand",
    "type-coverage": "type-coverage -p . --detail --strict --ignore-files **/*.test.tsx --ignore-files **/*.stories.tsx"
  },
  "keywords": [],
  "dependencies": {
    "writing-trainer-core": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/cli": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/chrome": "0.0.83",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "csv-loader": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^24.7.1",
    "lodash": "^4.17.11",
    "papaparse": "^4.6.3",
    "prettier": "^1.17.0",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "react-use": "^8.3.1",
    "sonarqube-scanner": "^2.4.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.3.0",
    "type-coverage": "^2.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "typeCoverage": {
    "atLeast": 100
  }
