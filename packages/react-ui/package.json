{
  "name": "writing-trainer-react-ui",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "sh helpers/build.sh",
    "build-storybook": "build-storybook",
    "check": "sh helpers/check.sh",
    "eslint": "eslint . --ext ts --ext tsx --ext js",
    "format": "sh helpers/format.sh",
    "lerna-link": "(cd ../.. && ./node_modules/.bin/lerna link)",
    "sonar": "npm test -- --coverage && SONAR_TOKEN=$(cat sonar-token.txt) node helpers/sonar.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest --runInBand",
    "type-coverage": "typescript-coverage-report --ignore-files '**/*.json'",
    "watch": "sh helpers/build.sh --watch"
  },
  "keywords": [],
  "dependencies": {
    "@testing-library/react": "^12.1.4",
    "webpack": "^5.70.0",
    "writing-trainer-core": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/cli": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.179",
    "@types/react": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "csv-loader": "^3.0.3",
    "eslint": "^8.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "jest": "^27.5.1",
    "lodash": "^4.17.15",
    "raw-loader": "^3.0.0",
    "sonarqube-scanner": "^2.5.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "typescript-coverage-report": "^0.6.4"
  },
  "typeCoverage": {
    "atLeast": 95
  }
}
