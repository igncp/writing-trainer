{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "module": "ES2015",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "outDir": "dist/js",
    "paths": {
      "#/*": ["src/frontend/*"]
    },
    "rootDir": ".",
    "sourceMap": false,
    "target": "es5",
    "typeRoots": ["node_modules/@types", "src/typings"]
  }
}
